<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<title>System1 CMP</title>
</head>
<body>

	<script>{{{ htmlWebpackPlugin.options.inline }}}</script>
	<script>
			// 1. can i initialize from outside?
			// how do i add init as a command or handle init pre-load and post-load
			cmp('init', {
				logging: true
			}, function(result) {
				console.log("init complete", result);
				if (!result.hasConsented) { // consent not set
					cmp('showConsentTool');
				}
			});

			cmp('addEventListener', 'onSubmit', function(result) {
			    console.log("onSubmit", result);
			});

			cmp('addEventListener', 'cmpReady', function(result) {
			    console.log("cmpReady", result);
			});
	</script>
</body>
</html>
